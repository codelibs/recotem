# Recotem Environment Configuration
# Copy this file to .env and update the values for your environment

# PostgreSQL Database Configuration
POSTGRES_USER=recotem_user
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_DB=recotem

# Django Secret Key (generate a secure random string)
# You can generate one with: python -c "import secrets; print(secrets.token_urlsafe(50))"
SECRET_KEY=your_secret_key_here

# Port to expose the application (optional, defaults to 80)
PORT=80
