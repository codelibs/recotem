{
  "name": "recotem-frontend",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "lint": "eslint src --fix",
    "type-check": "vue-tsc --noEmit",
    "generate:types": "openapi-typescript http://localhost:8000/api/v1/schema/ -o src/api/generated-types.ts"
  },
  "dependencies": {
    "@primevue/themes": "^4.5.0",
    "dayjs": "^1.11.13",
    "ofetch": "^1.4.1",
    "pinia": "^2.3.0",
    "primeicons": "^7.0.0",
    "primevue": "^4.5.0",
    "vue": "^3.5.13",
    "vue-i18n": "^11.2.8",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.1",
    "@playwright/test": "^1.49.0",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^22.10.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.16.0",
    "eslint-plugin-vue": "^10.8.0",
    "jsdom": "^28.0.0",
    "openapi-typescript": "^7.4.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.0",
    "vite": "^7.3.1",
    "vitest": "^2.1.0",
    "vue-tsc": "^2.2.0"
  }
}
